dollars = 0
percent = 0
def main():
    global dollars = dollars_to_float(input('How much was the meal?'))
    global percent = percent_to_float(input('What percentage would you like to tip?'))
    tip = dollars*percent
    print(f'Leave $(tip:.2f)')

def dollars_to_float(d)
    dollars = 'd'

