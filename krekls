def pasuti_tkreklus(skaits,apdruka,piegade):
    if apdruka==('TEKSTS'):
        print('Cena ir ',skaits*5,'eiro')
    elif apdruka==('ZIME'):
        print('Cena ir',skaits*7,'eiro')
    elif apdruka==('FOTO'):
        print('Cena ir',skaits*20,'eiro')
    else:
        print('Ievadiet drukas veidu: TEKSTS, ZIME vai FOTO')
    if bool(piegade)==True:
        print('+15')
    else:
        print('0')
print(pasuti_tkreklus(2,'ZIME',False))